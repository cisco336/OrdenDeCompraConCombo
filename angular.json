{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "FormularioOrdenDeCompra": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/FormularioOrdenDeCompra",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss",
              "node_modules/ngx-toastr/toastr.css"
            ],
            "scripts": []
          },
          "configurations": {
            "internet-dev": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/internet/environment.ts"
                }
              ]
            },
            "internet-qa": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/internet/environment.qa.ts"
                }
              ]
            },
            "internet-prod": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/internet/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "combo-dev": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/combo/environment.ts"
                }
              ]
            },
            "combo-qa": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/combo/environment.qa.ts"
                }
              ]
            },
            "combo-prod": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/combo/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "qa": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.qa.ts"
                }
              ],
              "optimization": false,
              "sourceMap": false,
              "extractCss": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "FormularioOrdenDeCompra:build"
          },
          "configurations": {
            "internet-dev": {
              "browserTarget": "FormularioOrdenDeCompra:build:internet-dev"
            },
            "internet-qa": {
              "browserTarget": "FormularioOrdenDeCompra:build:internet-qa"
            },
            "internet-prod": {
              "browserTarget": "FormularioOrdenDeCompra:build:internet-prod"
            },
            "combo-dev": {
              "browserTarget": "FormularioOrdenDeCompra:build:combo-dev"
            },
            "combo-qa": {
              "browserTarget": "FormularioOrdenDeCompra:build:combo-qa"
            },
            "combo-prod": {
              "browserTarget": "FormularioOrdenDeCompra:build:combo-prod"
            },
            "qa": {
              "browserTarget": "FormularioOrdenDeCompra:build:qa"
            },
            "production": {
              "browserTarget": "FormularioOrdenDeCompra:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "FormularioOrdenDeCompra:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "FormularioOrdenDeCompra:serve"
          },
          "configurations": {
            "internet-dev": {
              "devServerTarget": "FormularioOrdenDeCompra:serve:internet-dev"
            },
            "internet-qa": {
              "devServerTarget": "FormularioOrdenDeCompra:serve:internet-qa"
            },
            "internet-prod": {
              "devServerTarget": "FormularioOrdenDeCompra:serve:internet-prod"
            },
            "combo-dev": {
              "devServerTarget": "FormularioOrdenDeCompra:serve:combo-dev"
            },
            "combo-qa": {
              "devServerTarget": "FormularioOrdenDeCompra:serve:combo-qa"
            },
            "combo-prod": {
              "devServerTarget": "FormularioOrdenDeCompra:serve:combo-prod"
            },
            "qa": {
              "devServerTarget": "FormularioOrdenDeCompra:serve:qa"
            },
            "production": {
              "devServerTarget": "FormularioOrdenDeCompra:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "FormularioOrdenDeCompra"
}